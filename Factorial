
#include <iostream>  
#include <bits/stdc++.h>

using namespace std;
int main()            
{  
   int i,fact=1,number; 
   //enter any number
   
  cout<<"Enter any Number: ";    
 cin>>number;    //input
  for(i=1;i<=number;i++)    
  {    
      fact=fact*i;    
  }    
  cout<<"Factorial of " <<number<<" is: "<<fact<<endl;  
  return 0;  
}  
